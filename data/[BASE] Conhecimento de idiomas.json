    "idioma": "alemao",
    "tipo_conhecimento": "frase",
    "texto_original": "Guten Nacht.",
    "transcricao_ipa": "ˌɡuːtən ˈnaxt",
    "traducao": "Boa noite. [usado ao se despedir para ir dormir]",
    "divisao_silabica": "Gu-ten Nacht"
