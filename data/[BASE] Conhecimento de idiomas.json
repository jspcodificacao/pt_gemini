    "data_hora": "2025-10-07T10:20:00.000Z",
    "idioma": "alemao",
    "tipo_conhecimento": "frase",
    "texto_original": "Ich bin sechsundfünfzig Jahre alt.",
    "transcricao_ipa": "ɪç bɪn ˌzɛksʊntˈfʏnftsɪç ˈjaːʁə alt",
    "traducao": "Eu tenho cinquenta e seis anos de idade.",
    "divisao_silabica": "Ich bin sechs-und-fünf-zig Jah-re alt"
  },
  {
    "conhecimento_id": "b4c5d6e7-8f9a-0b1c-2d3e-4f5a6b7c8d9e",
    "data_hora": "2025-10-07T10:20:00.000Z",
    "idioma": "alemao",
    "tipo_conhecimento": "frase",
    "texto_original": "Ich bin ein Meter vierundsiebzig groß.",
