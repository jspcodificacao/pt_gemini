    "data_hora": "2025-10-07T10:20:00.000Z",
    "idioma": "alemao",
    "tipo_conhecimento": "frase",
    "texto_original": "Ich heiße Jucieudes.",
    "transcricao_ipa": "ɪç ˈhaɪsə ʒusiˈewdis",
    "traducao": "Eu me chamo Jucieudes.",
    "divisao_silabica": "Ich hei-ße Ju-ci-eu-des"
